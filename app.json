{
  "expo": {
    "name": "droute_user",
    "slug": "droute_user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#083c5d"
    },
    "ios": {
      "bundleIdentifier": "",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library.",
        "NSLocationWhenInUseUsageDescription": "We need your location to provide better services.",
        "NSLocationAlwaysUsageDescription": "We need continuous location access for tracking.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need continuous location access to provide real-time updates."
      }
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#083c5d"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyASRGQshp6t0wfi0WA-6MoHQsD0qAfflaM"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alokkumarsingh.droute_user"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "720c5d1b-2e78-407f-8634-f0f927ce0934"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
